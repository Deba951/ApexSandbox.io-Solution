public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	//code here
    Integer max;

    if((num1>num2 && num1>num3) || (num1>=num2 && num1>num3) || (num1>num2 && num1>=num3))
        max=num1;
    else if((num2>num1 && num2>num3) || (num2>=num1 && num2>num3) || (num2>num1 && num2>=num3))
        max=num2;
    else if((num3>num2 && num3>num1) || (num3>=num2 && num3>num1) || (num3>num2 && num3>=num1))
        max=num3;
    else
        return 0;

    return max;

}
